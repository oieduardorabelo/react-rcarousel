    "babel-eslint": "6.1.2",
    "babel-loader": "6.2.5",
    "babel-preset-babili": "0.0.1",
    "babel-preset-es2015": "6.14.0",
    "babel-preset-react": "6.11.1",
    "babel-preset-stage-0": "6.5.0",
    "css-loader": "0.24.0",
    "eslint": "3.4.0",
    "eslint-config-airbnb": "10.0.1",
    "eslint-plugin-immutable": "1.0.0",
